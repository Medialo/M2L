<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="540.0" prefWidth="789.0" style="-fx-background-color: #ffffff;"
            xmlns="http://javafx.com/javafx/8" fx:controller="fr.koala.m2l.controller.ButtonControler">
    <AnchorPane prefHeight="540.0" prefWidth="190.0" style="-fx-background-color: #757B78;"
                AnchorPane.bottomAnchor="0.0">
        <Button fx:id="refBtnWin" alignment="TOP_RIGHT" layoutX="-11.0" layoutY="244.0" mnemonicParsing="false"
                onMouseClicked="#onBtnAccueilPressed" prefHeight="31.0" prefWidth="164.0"
                style="-fx-background-color: #2C9170;" text="Accueil" textAlignment="RIGHT" textFill="WHITE">
            <font>
                <Font name="Calibri" size="14.0"/>
            </font>

        </Button>
        <Button fx:id="btnEquipes" alignment="TOP_RIGHT" contentDisplay="TOP" layoutX="-11.0" layoutY="364.0"
                mnemonicParsing="false" onMouseClicked="#onBtnEquipePressed" prefHeight="31.0" prefWidth="164.0"
                style="-fx-background-color: #2C9170;" text="Equipes" textAlignment="RIGHT" textFill="WHITE">
            <font>
                <Font name="Calibri" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="btnCompetitions" alignment="TOP_RIGHT" layoutX="-11.0" layoutY="284.0" mnemonicParsing="false"
                onMouseClicked="#onBtnCompPressed" prefHeight="31.0" prefWidth="164.0"
                style="-fx-background-color: #2C9170;" text="Compétitions" textAlignment="RIGHT" textFill="WHITE">
            <font>
                <Font name="Calibri" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="btnUsers" alignment="TOP_RIGHT" layoutX="-11.0" layoutY="324.0" mnemonicParsing="false"
                onMouseClicked="#onBtnUsersPressed" prefHeight="31.0" prefWidth="164.0"
                style="-fx-background-color: #2C9170;" text="Utilisateurs" textAlignment="RIGHT" textFill="WHITE">
            <font>
                <Font name="Calibri" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="btnParametre" alignment="TOP_RIGHT" layoutX="-11.0" layoutY="405.0" mnemonicParsing="false"
                onMouseClicked="#onBtnParamPressed" prefHeight="31.0" prefWidth="164.0"
                style="-fx-background-color: #2C9170;" text="Paramètres" textFill="WHITE">
            <font>
                <Font name="Calibri" size="14.0"/>
            </font>
        </Button>
        <Text fx:id="prenomText" fill="#f8f7f7" layoutX="5.0" layoutY="156.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="[Prenom] [Nom] " textAlignment="CENTER" wrappingWidth="181.0">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Text>
        <!-- <ImageView stylesheet="img.css" fitHeight="80.0" fitWidth="85.0" layoutX="37.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" styleClass="fr">
           <image>
              <Image url="@img/2x/outline_person_white_18dp.png" />
           </image>
        </ImageView> -->
        <Text fx:id="roleText" fill="#f8f7f7" layoutY="186.0" strokeType="OUTSIDE" strokeWidth="0.0" text="[Role] "
              textAlignment="CENTER" wrappingWidth="190.0">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Text>
        <Label layoutX="56.0" layoutY="506.0" opacity="0.54" text="Déconnexion" textFill="#dacfcf">
            <font>
                <Font name="System Italic" size="14.0"/>
            </font>
        </Label>
    </AnchorPane>
</AnchorPane>
